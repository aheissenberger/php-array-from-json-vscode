{
  "name": "php-array-from-json",
  "displayName": "PHP Array from/to JSON or JavaScript",
  "description": "Create PHP Array from/to JSON or JavaScript object notation Code.",
  "version": "0.0.1",
  "publisher": "heissenbergerlab",
  "license": "bsd-2-clause",
  "homepage": "https://github.com/aheissenberger/vscode-php-array-from-json-js",
  "repository": "https://github.com/aheissenberger/vscode-php-array-from-json-js.git",
  "engines": {
    "vscode": "^1.75.0"
  },
  "keywords": [
    "php",
    "array",
    "json",
    "converter",
    "javascript"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "php-array-from-json.fromJSON",
        "title": "JSON to PHP Array"
      },
      {
        "command": "php-array-from-json.fromJS",
        "title": "JS Object/Array to PHP Array"
      },
      {
        "command": "php-array-from-json.toJSON",
        "title": "JSON from PHP Array"
      }
    ],
    "configuration": {
      "title": "PHP Array from/to JSON",
      "properties": {
        "php-array-from-json.phpexec": {
          "type": "string",
          "default": "php",
          "markdownDescription": "Specifies the exact location of  `php` e.g. `/opt/bin/php` or use only the name `php` when available in `PATH`"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "vsce:package": "npx vsce package --no-yarn",
    "vsce:publish": "npx vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "php-array-from-json-js": "^0.0.7",
    "php-array-to-json-js": "^0.0.2"
  }
}
